@page
@model oficinaCovid.App.Frontend.Pages.PersonalAseoModel
@{
    ViewData["Title"] = "Personal de aseo";
}
<div class="col-md-8">
    <form action="" method="post">
        <input type="hidden" asp-for="gobernacion.id">
        <input type="hidden" asp-for="aseador.id">
        <div class="form-group">
            <label asp-for="aseador.identificacion">Identificación</label>
            <input type="text" asp-for="aseador.identificacion" class="form-control">
        </div>
        <div class="form-group">
            <label asp-for="aseador.nombres">Nombres</label>
            <input type="text" asp-for="aseador.nombres" class="form-control">
        </div>
        <div class="form-group">
            <label asp-for="aseador.apellidos">Apellidos</label>
            <input type="text" asp-for="aseador.apellidos" class="form-control">
        </div>
        <div class="form-group">
            <label asp-for="aseador.edad">Edad</label>
            <input type="number" asp-for="aseador.edad" class="form-control">
        </div>
        <div class="form-group">
            <label asp-for="aseador.genero">Genero</label>
            <select asp-for="aseador.genero" class="form-control">
                <option value="">Seleccione el genero</option>
                <option value="Femenino">Femenino</option>
                <option value="Masculino">Masculino</option>
            </select>
        </div>
        <div class="form-group">
            <label asp-for="aseador.horaIngreso">Hora de ingreso</label>
            <input type="time" asp-for="aseador.horaIngreso" class="form-control">
        </div>
        <div class="form-group">
            <label asp-for="aseador.horaSalida">Hora de salida</label>
            <input type="time" asp-for="aseador.horaSalida" class="form-control">
        </div>
        <button type="submit" class="btn btn-success">Guardar cambios</button>
    </form>
</div>
